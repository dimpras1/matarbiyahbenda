    <script>
      // Fungsi untuk scroll ke section yang tepat
      function scrollToSection(targetId) {
        const element = document.querySelector(targetId);
        if (element) {
          // Tunggu sebentar untuk memastikan semua animasi Alpine.js selesai
          setTimeout(() => {
            const offsetTop = element.offsetTop - 80; // 80px offset untuk fixed header
            window.scrollTo({
              top: offsetTop,
              behavior: "smooth",
            });
          }, 100);
        }
      }

      // Handle hash pada saat halaman dimuat
      document.addEventListener("DOMContentLoaded", function () {
        if (window.location.hash) {
          // Disable smooth scroll sementara
          document.documentElement.style.scrollBehavior = "auto";

          // Scroll ke posisi hash setelah delay untuk memastikan konten dimuat
          setTimeout(() => {
            scrollToSection(window.location.hash);
            // Re-enable smooth scroll
            document.documentElement.style.scrollBehavior = "smooth";
          }, 300);
        }
      });

      // Handle klik pada link hash (untuk navigasi dalam halaman yang sama)
      document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
        anchor.addEventListener("click", function (e) {
          const targetId = this.getAttribute("href");
          if (targetId !== "#" && document.querySelector(targetId)) {
            e.preventDefault();
            scrollToSection(targetId);
            // Update URL tanpa reload
            history.pushState(null, null, targetId);
          }
        });
      });
    </script>
